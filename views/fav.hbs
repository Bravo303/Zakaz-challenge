<div class="favContainer">
  <div>
    <h3 class="favTitle">Избранное</h3>
  </div>
  <div class="favSmallContainer">
    <div class="favTable">
      <table >
        {{#each fav}}
        <tr class='flextable'>
          <td> {{this.id}}Номер</td>
          <td><img class="small" src='image/Socks/{{this.favorites_link}}'>Фото</td>
          <td>{{this.id}}Описание</td>
        </tr>
        {{/each}}
        
      </table>
      <div class="favBtnContainer">
        <div class="">
          <img class="btnAddBasket" src="/image/кнопкаДобавитьВКорзину.png" alt="">
        </div>
        <div class="">
          <img class="btnShare" src="/image/кпонкаПоделиться.png" alt="">
        </div>
        <div class="">
          <img class="btnDeleteFromFav" src="/image/кнопка.убратьИзИзбранного.png" alt="">
        </div>


      </div>

    </div>

  </div>

  
</div>
